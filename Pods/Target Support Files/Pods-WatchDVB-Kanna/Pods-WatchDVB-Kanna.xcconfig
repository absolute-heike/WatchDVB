CONFIGURATION_BUILD_DIR = $PODS_FRAMEWORK_BUILD_PATH
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/Kanna" "${PODS_ROOT}/Headers/Public" $(SDKROOT)/usr/include/libxml2
OTHER_LDFLAGS = -l"xml2"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-WatchDVB
PODS_ROOT = ${SRCROOT}
SKIP_INSTALL = YES
SWIFT_INCLUDE_PATHS = $(SRCROOT)/Kanna/Modules